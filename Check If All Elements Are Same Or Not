#include<stdio.h>
#include<conio.h>

void main()
{
      int a[10],i,flag=0;
      clrscr();
      
      printf("Enter 10 Elements:\n");
      
      for(i=0;i<=9;i++)
      {
      scanf("%d",&a[i]);
      }
      
      for(i=0;i<=9;i++)
      {
        if(a[i]==a[i+1])
        {
         flag++;
        }
      }  
         if(flag==9)
         {
         printf("YES");
         }
         else
         {
         printf("NO");
         }
      
      
      getch();
      
 }