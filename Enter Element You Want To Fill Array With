

#include<stdio.h>
#include<conio.h>

void main()
{
      int a[10],i,id;
      clrscr();
      
      
      printf("Enter Index Number With Which You Want To Fill An Array\n");
      scanf("%d",&id);
      
      for(i=0;i<=9;i++)
      {
      a[i]=id;
      }
      
      for(i=0;i<=9;i++)
      {
      printf("\n%d\n",a[i]);
      }
      
      
      
      
      
      getch();
      
 }